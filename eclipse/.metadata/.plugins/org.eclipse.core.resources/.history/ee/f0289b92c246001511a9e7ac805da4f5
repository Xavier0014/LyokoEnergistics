package com.xavier0014.lyokoenergistics.creativetabs;

import com.xavier0014.lyokoenergistics.init.ModBlock;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class LECreativeTabs extends CreativeTabs{

	public LECreativeTabs(String lable) {
		super(lable);
	}
	
	@Override
	public Item getTabIconItem() {
		return Item.getItemFromBlock(ModBlock.supercomputer);
	}

}
